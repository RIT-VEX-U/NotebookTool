{
  "editorSuggest": {
    "enableAutoSuggest": true,
    "triggerPhrase": "//",
    "commentFormat": "html"
  },
  "decoration": {
    "autoRegisterLabels": true,
    "styles": {
      "labels": {
        "1737244013364": {
          "label": "note",
          "id": "1737244013364",
          "enableStyle": true,
          "style": {
            "color": "#00e4be",
            "italic": true,
            "fontWeight": "thin"
          }
        }
      },
      "tag": {
        "style": {
          "fontSize": 10,
          "opacity": 40
        },
        "enableStyle": true
      }
    },
    "defaultPalette": "bright"
  },
  "outline": {
    "fontSize": 12,
    "showLabelsFilter": true,
    "showSearchInput": true,
    "hiddenLabels": [],
    "hiddenTypes": []
  },
  "tts": {
    "rate": 1.1,
    "pitch": 1,
    "volume": 1,
    "focusAnnotationInEditor": true
  },
  "notes": {
    "defaultFolder": "notes",
    "notesNamingMode": "annotation-label/annotation-text",
    "openNoteAfterCreation": false,
    "insertLinkToNote": true,
    "defaultFolderMode": "current folder/notes/<file-name>",
    "template": "{{block_link}}",
    "truncateFileName": false
  },
  "idling": {
    "daysUnused": [
      "2025-02-24",
      "2025-02-25",
      "2025-02-26",
      "2025-03-02"
    ]
  },
  "clipboard": {
    "templates": {
      "front": "",
      "header": "# {{note_name}}",
      "highlight": "- {{highlight_text}}",
      "comment": "- _{{comment_text}}_"
    }
  }
}